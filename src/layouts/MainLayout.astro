---
import MainHead from "./MainHead.astro";
interface Props {
  title: string;
  description: string;
}
const { title, description } = Astro.props as Props;

import "../styles/global.css";
---

<html lang="en">
  <MainHead title={title} description={description} />
  <body>
    <slot> Default text</slot>
  </body>
</html>
